{
  "name": "tri-protocol-sdk",
  "version": "0.1.0-beta.1",
  "description": "Official SDK for Tri-Protocol Framework - Build sophisticated multi-agent systems with ease",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "build:bundle": "node build-bundle.js",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "docs": "typedoc --out docs/api src/index.ts",
    "prepublishOnly": "npm run clean && npm run test && npm run build",
    "preversion": "npm run test",
    "version": "npm run format "
  },
  "dependencies": {
    "eventemitter3": "^5.0.1",
    "uuid": "^9.0.1",
    "reflect-metadata": "^0.2.1"
  },
  "peerDependencies": {
    "@tri-protocol/core": "file:../core",
    "@tri-protocol/protocols": "file:../protocols",
    "@tri-protocol/logger": "file:../logger"
  },
  "peerDependenciesMeta": {
    "@tri-protocol/core": {
      "optional": true
    },
    "@tri-protocol/protocols": {
      "optional": true
    },
    "@tri-protocol/logger": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^24.0.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "sdk",
    "tri-protocol",
    "agents",
    "agentic",
    "automation",
    "ai",
    "orchestration",
    "multi-agent",
    "llm",
    "workflow",
    "a2a",
    "mcp",
    "langgraph"
  ],
  "author": "Tri-Protocol Team & @YmC",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AkiliAi/tri-protocol.git",
    "directory": "sdk"
  },
  "bugs": {
    "url": "https://github.com/AkiliAi/tri-protocol/issues"
  },
  "homepage": "https://github.com/AkiliAi/tri-protocol#readme",
  "engines": {
    "node": ">=18.0.0"
  }
}